<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFrameworks>net472;netcoreapp2.2;netcoreapp3.1</TargetFrameworks>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="MsgPack.Cli" version="1.0.1" />
    <PackageReference Include="Nerdbank.Streams" Version="2.4.48" />
    <PackageReference Include="Newtonsoft.Json" version="13.0.1" />
    <PackageReference Include="protobuf-net" version="3.0.101" />
    <PackageReference Include="System.CodeDom" Version="5.0.0" />
    <PackageReference Include="ZeroFormatter" version="1.6.4" Condition=" '$(TargetFramework)' != 'netcoreapp3.1' " />
    <PackageReference Include="Alexinea.ZeroFormatter" version="1.6.4" Condition=" '$(TargetFramework)' == 'netcoreapp3.1' " />
  </ItemGroup>
  <ItemGroup>
    <!-- Odin is not available on NuGet -->
    <Reference Include="..\..\benchmark\SerializerBenchmark\OdinSerializer.dll" Condition=" '$(TargetFramework)' == 'netcoreapp3.1' " />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\src\MessagePack\MessagePack.csproj" />
  </ItemGroup>
</Project>
